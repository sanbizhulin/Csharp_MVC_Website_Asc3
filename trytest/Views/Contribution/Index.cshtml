@model IEnumerable<trytest.Models.Contribution>

@{
    ViewBag.Title = "Index";
}

<h2>Index in Contribution</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ContributionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContributionQuantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contributiontype.ContributionTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ContributionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContributionQuantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.contributiontype.ContributionTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ContributionID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ContributionID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ContributionID })
        </td>
    </tr>
}

</table>
